{
  "name": "node-red-contrib-vban",
  "version": "0.9.0",
  "description": "Nodes to interact with vban protocol",
  "main": "build/nodes/vban.js",
  "scripts": {
    "debug": "yarn build && node-red -p 3002",
    "start": "node -r source-map-support/register ./build/test.js",
    "build": "yarn public && tsc -p tsconfig.json",
    "build:prod": "yarn public && tsc -p tsconfig.build.json",
    "public": "rimraf build && yarn copy:html",
    "copy:html": "copyfiles -E -V -u 1 \"./public/**/*\" build/nodes",
    "copy:examples": "copyfiles -E -V -u 1 \"./examples/**/*\" build/examples",
    "test": "jest",
    "test:CI": "jest --ci",
    "prepublishOnly": "npm run build:prod",
    "test:CI:coverage": "jest --ci --coverage",
    "watch": "jest --env=jsdom --watch",
    "prepare": "husky install",
    "lint": "eslint -f unix \"src/**\""
  },
  "keywords": [
    "node-red",
    "iot",
    "VBAN",
    "voicemeeter",
    "MT-32",
    "MT-64",
    "MT-128"
  ],
  "node-red": {
    "nodes": {
      "vban-server": "build/nodes/VBANServer.js",
      "vban-receiver": "build/nodes/VBANReceiver.js",
      "vban-sender": "build/nodes/VBANSender.js",
      "vban-udp-security": "build/nodes/UDPSecurity.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thib3113/node-red-contrib-vban"
  },
  "bugs": {
    "url": "https://github.com/thib3113/node-red-contrib-vban/issues"
  },
  "homepage": "https://github.com/thib3113/node-red-contrib-vban#readme",
  "author": {
    "email": "thib3113@gmail.com",
    "name": "Thibaut severac"
  },
  "license": "MIT",
  "dependencies": {
    "ip-address": "^8.1.0",
    "vban": "^1.0.6"
  },
  "devDependencies": {
    "@types/node": "16.11.9",
    "@types/node-red": "1.2.0",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "babel-eslint": "10.1.0",
    "copyfiles": "^2.4.1",
    "eslint": "8.3.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.4",
    "node-red": "next",
    "node-red-node-test-helper": "0.2.7",
    "prettier": "2.4.1",
    "rimraf": "^3.0.2",
    "source-map-support": "0.5.21",
    "ts-jest": "27.0.7",
    "ts-node": "10.4.0",
    "typescript": "4.5.2"
  },
  "engines": {
    "node": ">=10.22.1"
  },
  "files": [
    "/build",
    "/examples"
  ]
}
